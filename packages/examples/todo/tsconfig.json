{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": true,
		"outDir": "dist",
		"rootDir": ".",
		"allowJs": true,
		"resolveJsonModule": true
	},
	"ts-node": {
		"esm": true,
		"experimentalSpecifierResolution": "node"
	}
}
